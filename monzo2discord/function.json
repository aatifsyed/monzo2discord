{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "request",
      "methods": [
        "post",
        "get"
      ]
    },
    {
      "name": "discordfile",
      "type": "blob",
      "dataType": "binary",
      "path": "data/discord.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "monzofile",
      "type": "blob",
      "dataType": "binary",
      "path": "data/monzo.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "statein",
      "type": "blob",
      "dataType": "binary",
      "path": "data/state.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "stateout",
      "type": "blob",
      "dataType": "binary",
      "path": "data/state.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}